gleEssayView',\n    data() {\n        return {\n            essay: null,\n            loading: true,\n            errored: false,\n            user: this.$auth0.user,\n            isAuthenticated: this.$auth0.isAuthenticated,\n        }\n    },\n    methods: {\n        loadEssays() {\n            this.loading = true\n            axios\n                .get('http://localhost:5000/essay_db/' + this.$route.params.id)\n                .then(response => (this.essay = response.data))\n                .catch(error => {\n                    console.log(error)\n                    this.errored = true\n                })\n                .finally(() => {\n                    this.loading = false\n                })\n\n        },\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}