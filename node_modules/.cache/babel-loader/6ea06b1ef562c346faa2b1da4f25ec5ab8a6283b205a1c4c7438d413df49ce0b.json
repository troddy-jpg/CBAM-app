{"ast":null,"code":"import { toDisplayString as _toDisplayString, openBlock as _openBlock, createElementBlock as _createElementBlock } from \"vue\";\nexport function render(_ctx, _cache, $props, $setup, $data, $options) {\n  return _openBlock(), _createElementBlock(\"div\", null, \"Essay \" + _toDisplayString(_ctx.$route.params.id), 1 /* TEXT */);\n}","map":{"version":3,"mappings":";;uBACIA,oBAAuC,aAAlC,QAAM,oBAAGC,WAAM,CAACC,MAAM,CAACC,EAAE","names":["_createElementBlock","_ctx","params","id"],"sourceRoot":"","sources":["/Users/trevs/Schoolwork/CBAM-app/src/components/SingleEssayView.vue"],"sourcesContent":["<template>\n    <div>Essay {{ $route.params.id }}</div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport { useAuth0 } from '@auth0/auth0-vue';\n\nexport default {\n    auth0: useAuth0,\n    name: 'SingleEssayView',\n    data() {\n        return {\n            essays: null,\n            essay: essays[$route.params.id],\n            loading: true,\n            errored: false,\n            user: this.$auth0.user,\n            isAuthenticated: this.$auth0.isAuthenticated,\n        }\n    },\n    methods: {\n        loadEssay() {\n            this.loading = true\n            axios\n                .get('http://localhost:5000/essay_db')\n                .then(response => (this.essays = response.data))\n                .catch(error => {\n                    console.log(error)\n                    this.errored = true\n                })\n                .finally(() => this.loading = false)\n\n        },\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}