{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport { useAuth0 } from '@auth0/auth0-vue';\nexport default {\n  auth0: useAuth0,\n  name: 'EssaysView',\n  components: {},\n  data() {\n    return {\n      essays: [{} //{ id: -1, user_id: -1, title: \"\", content: \"\", revision: -1, format: \"\", date_modified: \"\" },\n      ],\n\n      errored: false,\n      loading: true,\n      user: this.$auth0.user,\n      isAuthenticated: this.$auth0.isAuthenticated,\n      newEssayTitle: null,\n      newEssayContent: null\n    };\n  },\n  mounted() {\n    this.reloadEssays();\n  },\n  methods: {\n    postEssay(title = \"\", content = \"\") {\n      if (!this.isAuthenticated) return; //don't post if not authenticated\n      axios.post('http://localhost:5000/essays', {\n        user_email: this.user.email,\n        title: title,\n        content: content\n      }).then(response => {\n        console.log(response);\n      }).finally(() => this.reloadEssays());\n    },\n    reloadEssays() {\n      if (!this.isAuthenticated) return; //don't make API call if not authenticated\n      this.loading = true;\n      axios.get('http://localhost:5000/essays').then(response => this.essays = response.data).catch(error => {\n        console.log(error);\n        this.errored = true;\n      }).finally(() => this.loading = false);\n    },\n    singleEssay(id) {\n      this.$router.push('/essay/' + id);\n    },\n    deleteEssay(id) {\n      axios.delete('http://localhost:5000/essay/' + id).then(response => {\n        console.log(response);\n      }).finally(() => this.reloadEssays());\n    }\n  }\n};","map":{"version":3,"mappings":";AAwBA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,kBAAkB;AAG3C,eAAe;EACXC,KAAK,EAAED,QAAQ;EACfE,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE,CAEZ,CAAC;EACDC,IAAI,GAAG;IACH,OAAO;MACHC,MAAM,EAAE,CACJ,CAAC,CAAC;MAAA,CACL;;MACDC,OAAO,EAAE,KAAK;MACdC,OAAO,EAAE,IAAI;MACbC,IAAI,EAAE,IAAI,CAACC,MAAM,CAACD,IAAI;MACtBE,eAAe,EAAE,IAAI,CAACD,MAAM