{"ast":null,"code":"import axios from 'axios';\nimport { useAuth0 } from '@auth0/auth0-vue';\nexport default {\n  auth0: useAuth0,\n  name: 'EssaysView',\n  components: {},\n  data() {\n    return {\n      essays: [{\n        id: -1,\n        user_id: -1,\n        title: \"\",\n        content: \"\",\n        revision: -1,\n        format: \"\",\n        date_modified: \"\"\n      }],\n      errored: false,\n      loading: true,\n      user: this.$auth0.user,\n      isAuthenticated: this.$auth0.isAuthenticated,\n      newEssayTitle: null,\n      newEssayContent: null\n    };\n  },\n  mounted() {\n    axios;\n    this.reloadEssays();\n  },\n  methods: {\n    postEssay(id = -1, title = \"\", content = \"\", toDelete = false) {\n      axios.post('http://localhost:5000/essay_db', {\n        id: id,\n        user_id: 1,\n        //this.user.email,\n        title: title,\n        content: content,\n        delete: toDelete\n      }).then(response => {\n        console.log(response);\n        this.reloadEssays();\n      });\n    },\n    reloadEssays() {\n      this.loading = true;\n      axios.get('http://localhost:5000/essay_db').then(response => this.essays = response.data).catch(error => {\n        console.log(error);\n        this.errored = true;\n      }).finally(() => this.loading = false);\n    }\n  }\n};","map":{"version":3,"mappings":"AA0BA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,kBAAkB;AAG3C,eAAe;EACXC,KAAK,EAAED,QAAQ;EACfE,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE,CAEZ,CAAC;EACDC,IAAI,GAAG;IACH,OAAO;MACHC,MAAM,EAAE,CACJ;QAAEC,EAAE,EAAE,CAAC,CAAC;QAAEC,OAAO,EAAE,CAAC,CAAC;QAAEC,KAAK,EAAE,EAAE;QAAEC,OAAO,EAAE,EAAE;QAAEC,QAAQ,EAAE,CAAC,CAAC;QAAEC,MAAM,EAAE,EAAE;QAAEC,aAAa,EAAE;MAAG,CAAC,CAC/F;MACDC,OAAO,EAAE,KAAK;MACdC,OAAO,EAAE,IAAI;MACbC,IAAI,EAAE,IAAI,CAACC,MAAM,CAACD,IAAI;MACtBE,eAAe,EAAE,IAAI,CAACD,MAAM,CAACC,eAAe;MAC5CC,aAAa,EAAE,IAAI;MACnBC,eAAe,EAAE;IACrB;EACJ,CAAC;EACDC,OAAO,GAAG;IACNrB,KAAI;IACA,IAAI,CAACsB,YAAY,EAAC;EAC1B,CAAC;EACDC,OAAO,EAAE;IACLC,SAAS,CAACjB,